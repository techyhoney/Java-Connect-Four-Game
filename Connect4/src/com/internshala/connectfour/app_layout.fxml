<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane fx:id="rootGridPane" style="-fx-background-color: #D9F7F0;" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.internshala.connectfour.Controller">
    <columnConstraints>
        <ColumnConstraints />
        <ColumnConstraints maxWidth="196.0" minWidth="160.0" prefWidth="196.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" />
        <RowConstraints />
    </rowConstraints>
    <children>
        <Pane GridPane.columnSpan="2" />
        <Pane fx:id="insertedDiscsPane" prefHeight="400.0" prefWidth="200.0" GridPane.rowIndex="1" />
        <VBox fx:id="vBox" style="-fx-background-color: #001933;" GridPane.columnIndex="1" GridPane.rowIndex="1">
            <children>
                <Region prefHeight="0.0" prefWidth="181.0" VBox.vgrow="ALWAYS" />
                <TextField fx:id="playerOneField" promptText="Name of Player 1">
                    <VBox.margin>
                        <Insets left="10.0" right="10.0" />
                    </VBox.margin>
                </TextField>
                <TextField fx:id="playerTwoField" layoutX="20.0" layoutY="40.0" promptText="Name of Player 2">
                    <VBox.margin>
                        <Insets bottom="5.0" left="10.0" right="10.0" />
                    </VBox.margin>
                </TextField>
                <Button fx:id="setNamesButton" alignment="CENTER" mnemonicParsing="false" prefHeight="26.0" prefWidth="182.0" text="Set Names" textAlignment="CENTER">
                    <VBox.margin>
                        <Insets bottom="30.0" left="10.0" right="10.0" />
                    </VBox.margin>
                </Button>
                <Label fx:id="playerNameLabel" alignment="CENTER" prefHeight="46.0" prefWidth="183.0" text="Player One" textFill="WHITE">
                    <font>
                        <Font name="System Bold" size="25.0" />
                    </font>
                    <padding>
                        <Insets left="10.0" right="10.0" top="10.0" />
                    </padding>
                    <VBox.margin>
                        <Insets />
                    </VBox.margin>
                </Label>
                <Label alignment="CENTER" contentDisplay="CENTER" layoutX="10.0" layoutY="174.0" prefHeight="20.0" prefWidth="184.0" text="Turn" textFill="WHITE">
                    <font>
                        <Font name="System Bold" size="14.0" />
                    </font>
                    <padding>
                        <Insets left="10.0" right="10.0" />
                    </padding>
                </Label>
                <Region prefHeight="124.0" prefWidth="181.0" VBox.vgrow="ALWAYS" />
            </children></VBox>
    </children>
</GridPane>
